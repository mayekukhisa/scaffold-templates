{
  "extends": ["expo", "plugin:prettier/recommended"],
  "plugins": ["prettier", "simple-import-sort"],
  "rules": {
    "no-restricted-imports": [
      "error",
      {
        "patterns": ["./*", "../*"]
      }
    ],
    "simple-import-sort/exports": "error",
    "simple-import-sort/imports": "error"
  }
}
